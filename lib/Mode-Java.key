sets HILIGHT_REGEXP
(
  (
   '\bbreak\b|\bcase\b|\bcontinue\b|\bdefault\b|\bexit\b|\bgoto\b|\bif\b|\breturn\b|\bswitch\b|\bwhile\b|\belse\b|\beach\b|\bimport\b|\
\bclass\b|\bmethod\b|\bpublic\b|\bprivate\b|\bprotected\b|\bextends\b|\btry\b|\bcatch\b|\bfinally\b|\bthrow\b'
   RESERVED_WORD_HILIGHT
  )
  (
   '\bchar\b|\buchar\b|\bint\b|\buint\b|\bbyte\b|\bubyte\b|\bfloat\b|\bdouble\b|\bconst\b|\bString\b|\bstatic\b|\bvoid\b'
   DATATYPE_HILIGHT
  )
  (
   '\b[Nn][Uu][Ll][Ll]\b|<t>|<nil>|\b[Tt][Rr][Uu][Ee]\b|\b[Ff][Aa][Ll][Ss][Ee]\b|\b[0-9]+\b'
   VALUE_LITERAL_HILIGHT
  )
  (
   '/\*.*\*/|/\*.*$|^[^/]+\*/|^\*/|//.*$'
   COMMENT_HILIGHT
  )
  (
   '"[^"]*"|\'[^\']*\''     ### "'
   STRING_LITERAL_HILIGHT
  )
);
setkey CTRL_I		smart-indent;
setkey CTRL_M		smart-newline;
setkey KEY_BTAB		smart-dedent;
