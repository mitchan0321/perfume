    setkey KEY_DC               delete-character;
    setkey KEY_SDC              delete-word-after-current-point;
    setkey kDC3                 delete-word;
    setkey kDC4                 <nil>;
    setkey kDC5                 delete-to-end-of-line;
    setkey kDC6                 <nil>;
    setkey kDC7                 <nil>;
    setkey kDC8                 <nil>;
    
    ### Press Home, Shift+Home, Alt+Home, Shift+Alt+Home, Control+Home
