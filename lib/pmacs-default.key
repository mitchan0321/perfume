###
### Prefix key define
###   format is: ( ((prefix-key ...) . "name") ... )
###
set-prefix (
    ((KEY_GS) . "C-]")
    ((KEY_GS CTRL_C) . "C-] C-c")
    ((KEY_GS CTRL_C CTRL_C) . "C-] C-c C-c")
    ((KEY_F10) . "[f10]")
);

setkey prefix: "C-]" CTRL_A test;
setkey prefix: "C-]" CTRL_B test;
setkey prefix: "C-]" ALT_a test;
setkey prefix: "C-]" "a" test;
setkey prefix: "C-]" KEY_F10 test;
setkey prefix: "C-] C-c" "a" test;
setkey prefix: "C-] C-c C-c" "a" test;
setkey prefix: "[f10]" KEY_F10 test;

